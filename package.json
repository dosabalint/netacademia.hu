{
  "name": "netacademia.hu",
  "version": "1.0.0",
  "description": "Teremtsd meg a jövőd!",
  "author": "puffy <nagy.gergo@3pixel.hu>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt generate",
    "prod": "npm run build && npm run start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "deploy:prod": "push-dir --dir=dist --branch=master --remote=https://github.com/NetAcademia/netacademia.github.io.git --cleanup",
    "deploy:gh-pages": "push-dir --dir=dist-ghpages --branch=gh-pages --cleanup",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/google-tag-manager": "^1.1.0",
    "nuxt": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.19.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.4.0",
    "push-dir": "^0.4.1"
  }
}
