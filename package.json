{
  "name": "netacademia.hu",
  "version": "1.0.0",
  "description": "Teremtsd meg a jövőd!",
  "author": "puffy <nagy.gergo@3pixel.hu>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev:ip": "nuxt --hostname 192.168.0.13",
    "generate:test": "cross-env-shell nuxt generate && echo 'test.netacademia.hu' > ./dist/CNAME",
    "generate:prod": "cross-env-shell NODE_ENV=production nuxt generate && echo 'www.netacademia.hu' > ./dist/CNAME",
    "deploy:test": "push-dir --dir=dist --branch=gh-pages",
    "deploy:prod": "push-dir --dir=dist --branch=master --remote=https://github.com/NetAcademia/netacademia.github.io.git",
    "build": "cross-env-shell nuxt build",
    "analyze": "cross-env-shell nuxt build --analyze",
    "start": "cross-env-shell nuxt start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@firebase/app": "^0.3.3",
    "@firebase/database": "^0.3.4",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-regular": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@nuxtjs/axios": "^5.3.1",
    "@nuxtjs/google-tag-manager": "^1.2.0",
    "babel-polyfill": "^6.26.0",
    "check-env": "^1.3.0",
    "dotenv": "^6.0.0",
    "gsap": "^2.0.1",
    "intersection-observer": "^0.5.0",
    "jquery-touchswipe": "^1.6.18",
    "npm": "^6.1.0",
    "nuxt": "^1.4.1",
    "nuxt-fontawesome": "^0.2.0",
    "vue-cookie-law": "^1.6.0",
    "vue-lazyload": "^1.2.6",
    "vue-mq": "^0.2.1",
    "vue-observe-visibility": "^0.3.1",
    "vue-parallaxy": "^1.1.1",
    "vue-youtube-embed": "^2.1.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-vue": "^4.5.0",
    "node-sass": "^4.9.0",
    "prettier": "^1.13.5",
    "push-dir": "^0.4.1",
    "sass-loader": "^7.0.3"
  }
}
