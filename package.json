{
  "name": "netacademia.hu",
  "version": "1.0.0",
  "description": "Teremtsd meg a jövőd!",
  "author": "puffy <nagy.gergo@3pixel.hu>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev:prod": "NODE_ENV=production nuxt",
    "build": "nuxt build",
    "analyze": "nuxt build --analyze",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "generate:test": "npm run generate && echo 'test.netacademia.hu' > ./dist/CNAME",
    "prod": "npm run build && npm run start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "deploy:prod": "push-dir --dir=dist --branch=master --remote=https://github.com/NetAcademia/netacademia.github.io.git --cleanup",
    "deploy:test": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.1",
    "@nuxtjs/google-tag-manager": "^1.1.0",
    "intersection-observer": "^0.5.0",
    "npm": "^6.0.1",
    "nuxt": "^1.0.0",
    "vue-mq": "^0.2.1",
    "vue-observe-visibility": "^0.3.1",
    "vue-youtube-embed": "^2.1.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.19.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.4.0",
    "push-dir": "^0.4.1"
  }
}
